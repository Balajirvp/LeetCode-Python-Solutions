# Solution 1: (84 ms	14.1 MB)

class Solution:
    def findMedianSortedArrays(self, nums1: List[int], nums2: List[int]) -> float:
        new_list = sorted(nums1 + nums2)
        l = len(new_list)
        if l % 2:
            return new_list[l//2]
        else:
            return (new_list[int(l/2)] + new_list[int(l/2) - 1])/2
            

